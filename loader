-- vxpt loader

-- Loadsting & UI
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("VXPT LOADER", "Synapse")

-- Main
local Main = Window:NewTab("Menu")
local MSection = Main:NewSection("Credits: vene_pl#7777")

-- First
local First = Window:NewTab("Script Hubs")
local FSection = First:NewSection("Domain X")
FSection:NewButton("Open Hub", "Execute DomainX Hub", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/DomainX/main/source',true))()
end)
local FSection = First:NewSection("C00lGUI")
FSection:NewButton("Open Hub", "Execute C00lGUI", function()
    loadstring(game:GetObjects("rbxassetid://9827584846")[1].Source)()
end)
local FSection = First:NewSection("Nullware")
FSection:NewButton("Open Hub", "Execute Nullware", function()
    getgenv().Theme = "Blue" -- To change the UI Theme, set this to one of the following options: "Red", "Purple", "Blue", "Green", "Yellow"
loadstring(game:HttpGet("https://gist.githubusercontent.com/M6HqVBcddw2qaN4s/2d722888a388017c18028cd434c43a25/raw/dcccf1027fe4b90780e47767aaf584389c9d7771/EULma3fU90PUOKUn?identifier=".. (function()local a=""for b=1,256 do local c=math.random(1,3)a=a..string.char(c==1 and math.random(48,57)or c==2 and math.random(97,122)or c==3 and math.random(65,90))end;return a end)()))()
end)

-- Second
local Second = Window:NewTab("Scripts")
local SSection = Second:NewSection("Project New World")
SSection:NewButton("Open Script", "Execute PNW Script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ZaRdoOx/Platinium/main/Loader"))()
end)
local SSection = Second:NewSection("Fly Race")
SSection:NewButton("Open Script", "Execute FR Script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/1201for/littlegui/main/Bla_Race'))()
end)
local SSection = Second:NewSection("Jailbreak")
SSection:NewButton("Open Script", "Execute JB Script", function()
    loadstring(game:HttpGet('https://scripts.luawl.com/14245/JailbreakerFree.lua'))()
end)
local SSection = Second:NewSection("Minerscave")
SSection:NewButton("Open Script", "Execute MC Script", function()
    loadstring(game:HttpGet("https://gist.githubusercontent.com/raw/b8d379c1e296ade8305c2fe4df652537"))()
end)

-- Third
local Third = Window:NewTab("Misc")
local TSection = Third:NewSection("Miscellaneous")
TSection:NewButton("Anti AFK", "Execute AntiAFK Script", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/JCBk5ieD"))()
end)
TSection:NewToggle("Speed 100", "Toggle Speed 100", function(state)
    if state then
        getgenv().WalkSpeedValue = 100;
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
end)
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
    else
        getgenv().WalkSpeedValue = 16;
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
end)
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
    end
end)
TSection:NewToggle("Jump 100", "Toggle Jump 100", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
    else
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)
local TSection = Third:NewSection("Avatar")
TSection:NewButton("Headless", "Be Headless", function()
    loadstring(Game:HttpGet("https://pastebin.com/raw/aFzX2pwi"))()
end)
TSection:NewButton("FE Snake", "Become a Snake", function()
    loadstring(game:HttpGet(('https://pastefy.ga/tWBTcE4R/raw'),true))()
end)
TSection:NewButton("Fe Flying UFO", "Flying UFO made by quirky anime boy#7003", function()
    loadstring(Game:HttpGet("https://pastebin.com/raw/8VGUmgUb"))()
end)
